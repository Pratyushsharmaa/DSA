//Array - 10,20,30
//All pairs pattern 2 - (10,10),(10,20),(10,30), (20,20), (20,30),(30,30)

#include<iostream>
using namespace std;

int printAllPairs(int arr[], int size);

int main(){
    int arr[] = {10,20,30};
    int size = 3;
    printAllPairs(arr,size);



    return 0;

}

int printAllPairs(int arr[], int size){
    for(int i=0; i<size;i++){
        for(int j=i;j<size;j++){
            cout<<arr[i]<<","<<arr[j]<<endl;
        }
    }
}
